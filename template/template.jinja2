<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Student manager{% endblock %}</title>
        <script src="{{ base_url }}/static/js/jquery.min.js"></script>
        <script src="{{ base_url }}/static/js/jquery-ui.js"></script>
        <script src="{{ base_url }}/static/js/jquery.form.js"></script>
        <script src="{{ base_url }}/static/js/bootstrap.min.js"></script>

        <link rel="stylesheet" href="{{ base_url }}/static/css/bootstrap.css">
        <link rel="stylesheet" href="{{ base_url }}/static/css/bootstrap-theme.css">
        <link rel="stylesheet" type="text/css" media="all" href="{{ base_url }}/static/css/jquery-ui.css"/>
        <link rel="stylesheet" type="text/css" href="{{ base_url }}/static/css/generic.css"/>

        {% block head %}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            {% block content %}
            {% endblock %}
        </div>
        <footer class="navbar navbar-default navbar-fixed-bottom">
            <div class="container-fluid">
                <p class="navbar-text pull-right">This bot is unaffiliated with Discord</p>
            </div>
        </footer>
        {% block script %}
        {% endblock %}
    </body>
</html>